#include <stdio.h>
#include <stdlib.h>
void firs_func() {
    int true_or_false = 1;
    char auxillaury_c;
        int f_but;
           if (scanf("%d%c", length, &auxillaury_c) == 2 && auxillaury_c == '\n' && *length > 0) {
               for (int i = 0; i < *length; i++)  {
                   if (scanf("%d%c", &f_but, &auxillaury_c) == 2 &&
                       (auxillaury_c == '\n' || auxillaury_c == ' ')) {
                       buffer[i] = f_but;
                   }
               }
           }
}
void second_func() {
    int help;
    int lenght;
    int hight;
    scanf("%d",&lenght);
    scanf("%d", &hight);
    int** single_array_matrix = malloc(lenght*hight*sizeof(int) + lenght*sizeof(int*));
    int* ptr = (int*)(single_array_matrix + lenght);
    
    for (int i = 0; i < lenght; i++) {
        single_array_matrix[i] = ptr + hight*i;
    }
    for (int i = 0;i < lenght; i++) {
        for (int j = 0; j < hight; j++) {
            if (scanf("%d%c", &help, &auxillaury_c) == 2 &&
                            (auxillaury_c == '\n' || auxillaury_c == ' ')) {
            single_array_matrix[i][j] = help
        }
    }
}
    for (int i = 0; i < lenght; i++) {
        for (int j = 0; j < hight; j++) {
            printf("%d ", single_array_matrix[i][j]);
        }
    }
    free(single_array_matrix);
}
void third_func() {
    int help;
    int lenght;
    int hight;
    scanf("%d",&lenght);
    scanf("%d", &hight);
    int** pointer_array = malloc(lenght*sizeof(int*));
    for(int i = 0; i < lenght; i++) {
    pointer_array[i]=malloc(hight*sizeof(int));
    }
    for (int i = 0;i < lenght; i++) {
        for (int j = 0; j < hight; j++) {
            if (scanf("%d%c", &help, &auxillaury_c) == 2 &&
                            (auxillaury_c == '\n' || auxillaury_c == ' ')) {
                pointer_array[i][j] = help
        }
    }
}
    for (int i = 0; i < lenght; i++) {
        for (int j = 0; j < hight; j++) {
            printf("%d ", pointer_array[i][j]);
        }
    }
    for (int i = 0; i < lenght; i++) {
        free(pointer_array[i]);
    }
    free(pointer_array);
}
void fourth_func() {
    int help;
    int lenght;
    int hight;
    scanf("%d",&lenght);
    scanf("%d", &hight);
    int** pointer_array = malloc(lenght*sizeof(int*));
    int* values_array = malloc (lenght*hight*sizeof(int));
    for(int i = 0; i < lenght; i++) {
    pointer_array[i]=values_array + hight*i;
    }
    for (int i = 0;i < lenght; i++) {
        for (int j = 0; j < hight; j++) {
            if (scanf("%d%c", &help, &auxillaury_c) == 2 &&
                            (auxillaury_c == '\n' || auxillaury_c == ' ')) {
                pointer_array[i][j] = help
        }
    }
}
    for (int i = 0; i < lenght; i++) {
        for (int j = 0; j < hight; j++) {
            printf("%d ", pointer_array[i][j]);
        }
    }
    free(values_array);
    free(pointer_array);
}
void menu() {
    char c
    printf("Please,enter type of memore allocation: ");
    printf("1.Static\n2.First dynamic\n3.Seconf dynamic\n4.Third dynamic");
    scanf("%d%c", n, &c);
     if (c != '\n' || *n !=1) {
       flag = 0;
     } else {
         if (c == 1) {
             int size = size_of_array();
             int data[size];
             if (staticinput(data,size) == 1 {
                 output(data,size);
             }
         } else if (c == 2) {
                 second_func();
         } else if (c == 3) {
             third_func();
         } else if (c == 4) {
             fourth_func();
         }
     }
}

int main() {
    int size = size_of_array();
    int *start = (int*)malloc(size * sizeof(int));
    if (input(start, size) == 1) {
        b_sort(start,size);
        output(start,size);
    } else {
        printf("n/a");
    }
    free(start);
    return 0;
}
